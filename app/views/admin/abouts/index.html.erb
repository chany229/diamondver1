<p id="notice"><%= notice %></p>

<h1>管理设置</h1>

<div class="content_wrapper">
  <div class="content">
<table>
  <thead>
    <tr>
      <th>首页关于</th>
      <th>联系方式</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @abouts.each do |about| %>
      <tr>
        <td><%= about.about %></td>
        <td><%= about.contact %></td>
        <td><%= link_to '预览', [:admin, about] %></td>
        <td><%= link_to '编辑', edit_admin_about_path(about) %></td>
        <td><%= link_to '删除', [:admin, about], method: :delete, data: { confirm: '确认删除？' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to '新建设置项', new_admin_about_path %>
</div>
</div>